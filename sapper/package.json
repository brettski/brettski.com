{
  "name": "brettski.com",
  "description": "Another attempt for a consolidated brettski site",
  "version": "5.0.0",
  "scripts": {
    "dev:sapper": "sapper dev",
    "dev:tailwindcss": "postcss static/tailwind.css -o static/main.css -w",
    "dev": "concurrently npm:dev:sapper npm:dev:tailwindcss",
    "build:orig": "sapper build --legacy",
    "export:orig": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "build:tailwindcss": "NODE_ENV=production postcss static/tailwind.css -o static/main.css",
    "build": "npm run build:tailwindcss && sapper build --legacy",
    "export": "npm run build:tailwindcss && sapper export --legacy"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "compression": "^1.7.1",
    "install": "^0.13.0",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@jackfranklin/rollup-plugin-markdown": "^0.2.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "concurrently": "^5.3.0",
    "dayjs": "^1.8.33",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "rollup": "^2.3.4",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^6.1.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "tailwindcss": "^1.6.2"
  }
}
