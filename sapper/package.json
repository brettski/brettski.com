{
  "name": "brettski.com",
  "description": "Another attempt for a consolidated brettski site",
  "version": "5.0.0",
  "scripts": {
    "dev:sapper": "sapper dev",
    "dev:tailwindcss": "postcss static/tailwind.css -o static/main.css -w",
    "dev": "concurrently npm:dev:sapper npm:dev:tailwindcss",
    "build:orig": "sapper build --legacy",
    "export:orig": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "build:tailwindcss": "NODE_ENV=production postcss static/tailwind.css -o static/main.css",
    "build": "npm run build:tailwindcss && sapper build --legacy",
    "export": "npm run build:tailwindcss && sapper export --legacy"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "compression": "^1.7.4",
    "install": "^0.13.0",
    "polka": "0.5.2",
    "sirv": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "@jackfranklin/rollup-plugin-markdown": "^0.2.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@tailwindcss/ui": "^0.5.0",
    "concurrently": "^5.3.0",
    "dayjs": "^1.8.33",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "plugin": "^0.3.3",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.26.3",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.24.1",
    "tailwindcss": "^1.6.2"
  }
}
